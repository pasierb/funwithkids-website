---
import { SITE_CONFIG } from '~/utils/config';
import logoImage from "~/assets/images/fwk.png";
---
<style>
  img {
    height: 1.5em;
    margin-right: 0.3em;
    display: inline-block;
  }
</style>
<span class="self-center ml-2 text-2xl md:text-xl font-bold text-gray-900 whitespace-nowrap dark:text-white">
  <img src={logoImage.src} class="text-3xl" /> {SITE_CONFIG?.name}
</span>
